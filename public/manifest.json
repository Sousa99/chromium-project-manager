{
  "manifest_version": 3,
  "short_name": "Chromium Project Manager",
  "name": "Chromium Project and Ticket Manager",
  "description": "A chromium extension to track project and tickets for work",
  "version": "0.1.1",

  "permissions": ["activeTab", "storage"],
  "action": {
    "default_popup": "index.html",
    "default_title": "Open up project manager"
  },
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  }
}
